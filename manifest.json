{
  "name": "NJIT Crummy UI Helper",
  "version": "2.1",
  "description": "Hacks NJIT UI for more functionality",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Input What-If",
    "default_popup": "views/popup.html"
  },
  "manifest_version": 2,
  "options_page": "views/options.html",
  "permissions": [
    "background",
    "storage",
    "activeTab",
    "https://dwapppr1.njit.edu/*",
    "http://myhub.njit.edu/*"
  ],
  "icons": {
    "16": "icons/dw_icon_16.png",
    "32": "icons/dw_icon_32.png",
    "48": "icons/dw_icon_48.png",
    "128": "icons/dw_icon_128.png"
  }
}