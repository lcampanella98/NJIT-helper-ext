{
  "name": "NJIT UI Helper",
  "version": "1.3.0",
  "description": "Add features to NJIT Websites",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "NJIT UI Actions",
    "default_popup": "views/popup.html"
  },
  "manifest_version": 2,
  "options_page": "views/options.html",
  "permissions": [
    "background",
    "storage",
    "activeTab",
    "tabs",
    "https://dwapppr1.njit.edu/*",
    "http://myhub.njit.edu/*",
    "https://reslifecinema-njit-edu.swankmp.net/*"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "short_name": "NJIT UI"
}